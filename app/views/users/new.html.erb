<h1>Create Your Account</h1>

<%= form_with model: @user, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :age %>
    <%= f.number_field :age%>
    <%= f.label :male %>
    <%= f.radio_button :gender, :male %>
    <%= f.label :female %>
    <%= f.radio_button :gender, :female %>
    <%= f.label :dob %>
    <%= f.date_field :dob %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "create profile"%>
<% end %>